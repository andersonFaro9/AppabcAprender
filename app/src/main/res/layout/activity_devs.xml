<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:paddingLeft="0dp"
              android:paddingTop="0px"
              android:paddingRight="0dp"
              android:baselineAligned="false">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:text="Desenvolvedores:"
            android:layout_width="280dp"
            android:layout_height="35dp"
            android:id="@+id/textView9"
            android:textSize="25sp"
            android:textAlignment="viewStart"
            android:textStyle="normal|bold"
            android:layout_marginEnd="64dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="64dp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toTopOf="parent"
            />

        <TextView
            android:text="O aplicativo foi desenvolvido por Anderson Faro Santana e Lucas Barbosa, responsável pelo designer. Para entrar em contato, é só falar pelo Twitter ou Behance:"
            android:layout_width="323dp"
            android:layout_height="78dp"
            android:id="@+id/textView10"
            android:textSize="15sp"
            android:textAlignment="viewStart"
            tools:layout_editor_absoluteX="16dp"
            android:layout_marginTop="64dp"
            app:layout_constraintTop_toTopOf="parent"/>


        <TextView
            android:text="Contatos:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/contatos"
            android:textSize="25sp"

            android:textStyle="normal|bold"
            android:textAlignment="viewStart"
            android:layout_marginEnd="240dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="240dp"
            android:layout_marginTop="168dp"
            app:layout_constraintTop_toTopOf="parent"/>


        <TextView
            android:text="twitter.com/andersonFaro9"
            android:layout_width="218dp"
            android:autoLink="web"
            android:textColorLink="@color/background_white"
            android:layout_height="23dp"
            android:id="@+id/andersonfaro9"
            android:textSize="15sp"
            android:layout_marginTop="224dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="128dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="128dp"
            />

        <TextView
            android:text="behance.net/Lucas-Barbosa"
            android:autoLink="web"
            android:textColorLink="@color/background_white"
            android:layout_width="203dp"
            android:layout_height="20dp"
            android:id="@+id/textView8"
            android:textSize="15sp"
            android:textAlignment="viewStart"
            android:layout_marginEnd="144dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="144dp"
            android:layout_marginTop="200dp"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:layout_width="345dp"
            android:layout_height="142dp"
            app:srcCompat="@drawable/ic_code"
            android:id="@+id/ic_code"
            android:scaleType="center"
            tools:layout_editor_absoluteX="16dp"
            android:layout_marginTop="296dp"
            app:layout_constraintTop_toTopOf="parent"/>



        <TextView
            android:text="Repositório: https://goo.gl/6iQXf6"
            android:autoLink="web"
            android:textColorLink="@color/background_white"
            android:layout_width="331dp"
            android:layout_height="37dp"
            android:id="@+id/repositorio"
            android:textSize="15sp"
            android:layout_marginTop="456dp"
            app:layout_constraintTop_toTopOf="parent"
            android:textAlignment="viewStart"
            tools:layout_editor_absoluteX="16dp"/>

    </android.support.constraint.ConstraintLayout>




</LinearLayout>